{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-undef": "warn",
        "no-param-reassign": "off",
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true
    }
}
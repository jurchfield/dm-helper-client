{
  "name": "dm-toolbox",
  "version": "0.1.0",
  "contributors": [
    "John Burchfield"
  ],
  "license": "MIT",
  "repository": "jurchfield/dm-helper",
  "scripts": {
    "start": "polymer serve",
    "build": "polymer build",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/lit-element": "^0.6.2",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-tabs": "^3.0.1",
    "@polymer/polymer": "^3.0.0",
    "@vaadin/vaadin-combo-box": "^4.2.0",
    "@vaadin/vaadin-dialog": "^2.2.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "pwa-helpers": "^0.9.0"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.7.6",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.2.0",
    "puppeteer": "^1.5.0",
    "wct-browser-legacy": "^1.0.1"
  }
}
